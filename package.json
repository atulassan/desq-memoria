    "build": "npm run build:ui && npm run build:server",
    "start1": "set debug=* && ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "prod": "tsc && node ./build/app.js",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
